#include <iostream>
#include <cstring>

using namespace std;

class Solution{
bool isNum(char c){
  return (c >= '0' && c <= '9') ? true : false;
}


int myAtoi(char * str){
  if(str == NULL)
    return 0;
  int sign = 1;
  int res = 0;
  if(str[0] == '-')  sign = -1;
  for(int i = 1; str[i] != "\0"; ++i){
    char c = str[i];
    if(isNum(c) == false)
      return sign*res; // handle the specifical case;
    res += 10*res + str[i] - '0';
  }

  return sign*res;
}
};

int main(int argc, char[] * argc){
  Solution S;
  for(int i = 1; i < argc; ++i)
    cout<<argc[i]: <<S.myAtoi(argc[i])<<"\t"<<atoi(argc[i])<<endl;
  return 0;

}
